{
	"name": "cesil-lang",
	"displayName": "Cesil programming language",
	"description": "Add cesil language support",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "cesil",
			"aliases": ["Cesil", "cesil"],
			"extensions": [".ces"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "cesil",
			"scopeName": "source.ces",
			"path": "./syntaxes/cesil.tmLanguage.json"
		}]
	},
	"publisher": "danik4985",
	"repository": {
		"type": "git",
		"url": "https://github.com/danik4985/cesil-vscode.git"
	}
}